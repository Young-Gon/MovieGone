{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "npm: android",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/.bin/react-native",
      "args": ["run-android"],
      "console": "integratedTerminal",
